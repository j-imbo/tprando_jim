{
  "tracker_right_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "options"
              },
              
              "content": {
                "type": "layout",
                "key": "item_grid_wide"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "vertical",
                "compact": true
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "tabbed",
              "dock": "top",
              "tabs": [
                {
                  "title": "Overworld",
                  "icon": "images/map.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "overworld"
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "tracker_top_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "left",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "options"
              },
              "content": {
                "type": "layout",
                "key": "item_grid_wide"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "style": "wrap",
                "type": "recentpins",
                "orientation": "horizontal",
                "compact": true
              }
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Overworld",
              "icon": "images/map.png",
              "content": {
                "type": "map",
                "maps": [
                  "overworld"
                ]
              }
            }
          ]
        }
      ]
    }
  }
}